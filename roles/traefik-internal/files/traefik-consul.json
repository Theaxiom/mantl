{
  "services": [{
    "name": "traefik-internal",
    "id": "traefik-http",
    "tags": ["http"],
    "port": 82,
    "check": {
      "script": "curl --silent --show-error --fail --dump-header /dev/stderr --retry 2 --connect-timeout 5 --max-time 5 http://localhost:82/"
    }
  }, {
    "name": "traefik-internal-admin",
    "port": 8082,
    "checks": [{
      "id": "web-ui",
      "name": "web ui availability",
      "script": "curl --insecure --silent --show-error --fail --dump-header /dev/stderr --retry 2 --connect-timeout 5 --max-time 5 https://localhost:8082/health",
      "interval": "10s",
      "timeout": "2s"
    }, {
      "id": "api",
      "name": "api contains information",
      "script": "if [[ \"$(curl --silent --insecure --retry 2 --connect-timeout 5 --max-time 5 https://localhost:8082/api)\" == \"{}\" ]]; then echo API contains no information. If this is not expected, try restarting traefik.; exit 1; fi;",
      "interval": "10s",
      "timeout": "2s"
    }]
  }]
}
